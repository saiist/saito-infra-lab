{
    "family": "saito-infra-lab-dev-app",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "executionRoleArn": "arn:aws:iam::977099016337:role/saito-infra-lab-dev-ecs-exec",
    "taskRoleArn": "arn:aws:iam::977099016337:role/saito-infra-lab-dev-ecs-task",
    "containerDefinitions": [
        {
            "name": "app",
            "image": "REPLACED_BY_CI",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "ENV",
                    "value": "dev"
                },
                {
                    "name": "PORT",
                    "value": "8080"
                },
                {
                    "name": "DB_HOST",
                    "value": "saito-infra-lab-dev-pg.cfuwca6iqsoe.ap-northeast-1.rds.amazonaws.com"
                },
                {
                    "name": "DB_PORT",
                    "value": "5432"
                },
                {
                    "name": "DB_NAME",
                    "value": "app"
                }
            ],
            "secrets": [
                {
                    "name": "DB_SECRET_JSON",
                    "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:977099016337:secret:saito-infra-lab/dev/db-miicGw"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/saito-infra-lab/dev/app",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "app"
                }
            }
        }
    ]
}